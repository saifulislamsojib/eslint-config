{
  "name": "eslint",
  "version": "0.0.1",
  "description": "eslint",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --max-warnings 0 --fix",
    "format": "prettier --log-level warn --write \"./**/*.{js,mjs,cjs,ts,d.ts,mts,json,yaml,yml,html}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky || true"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/saifulislamsojib/eslint"
  },
  "author": "https://github.com/saifulislamsojib",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@eslint/js": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0"
  },
  "devDependencies": {
    "eslint": "^9.30.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.35.1"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "eslint-import-resolver-typescript": ">=4",
    "eslint-plugin-jsx-a11y": ">=6",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=5",
    "prettier": ">=3",
    "typescript-eslint": ">=8"
  },
  "peerDependenciesMeta": {
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,d.ts,mts}": "eslint --max-warnings 0 --fix",
    "*.{js,mjs,cjs,ts,d.ts,mts,json,yaml,yml,html}": "prettier --log-level warn --write"
  }
}
