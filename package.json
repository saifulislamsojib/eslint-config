{
  "name": "eslint",
  "version": "0.0.1",
  "description": "eslint",
  "type": "module",
  "scripts": {
    "lint": "eslint --max-warnings 0 --fix",
    "format": "prettier --log-level warn --write \"./**/*.{js,mjs,cjs,ts,d.ts,mts,json,yaml,yml,html}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky || true"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/saifulislamsojib/eslint"
  },
  "author": "https://github.com/saifulislamsojib",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-prefer-function-component": "^4.0.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.39.0"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,d.ts,mts}": "eslint --max-warnings 0 --fix",
    "*.{js,mjs,cjs,ts,d.ts,mts,json,yaml,yml,html}": "prettier --log-level warn --write"
  }
}
