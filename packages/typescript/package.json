{
  "name": "@stack-lint/typescript",
  "version": "0.0.3",
  "description": "@stack-lint/typescript",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "default": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "prepublish": "cd .. && pnpm lint-staged",
    "generate": "npx -p typescript tsc index.js --declaration --allowJs --emitDeclarationOnly --outDir ."
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "stack-lint",
    "typescript-eslint",
    "stack-lint-typescript",
    "@stack-lint/typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saifulislamsojib/eslint-config"
  },
  "author": "https://github.com/saifulislamsojib",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint-import-resolver-typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@stack-lint/base": "0.0.3",
    "eslint": ">=9",
    "prettier": ">=3",
    "typescript": ">=5.6"
  },
  "engines": {
    "node": ">=20"
  }
}
