{
  "name": "@stack-lint/base",
  "version": "0.0.2",
  "description": "@stack-lint/base",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./web": {
      "import": "./web/index.js",
      "require": "./web/index.js",
      "types": "./web/index.d.ts"
    }
  },
  "scripts": {
    "prepublish": "cd .. && pnpm lint-staged",
    "generate": "npx -p typescript tsc **/*.js --declaration --allowJs --emitDeclarationOnly --outDir ."
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "stack-lint",
    "base-eslint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saifulislamsojib/eslint-config"
  },
  "author": "https://github.com/saifulislamsojib",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.4"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "prettier": ">=3",
    "globals": ">=16"
  },
  "peerDependenciesMeta": {
    "globals": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  }
}
