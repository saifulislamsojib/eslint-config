{
  "name": "@stack-lint/node",
  "version": "0.0.2",
  "description": "@stack-lint/node",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "prepublish": "cd .. && pnpm lint-staged",
    "generate": "npx -p typescript tsc index.js --declaration --allowJs --emitDeclarationOnly --outDir ."
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "stack-lint",
    "node-eslint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saifulislamsojib/eslint-config"
  },
  "author": "https://github.com/saifulislamsojib",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "globals": "^16.3.0"
  },
  "peerDependencies": {
    "@stack-lint/base": "0.0.2",
    "eslint": ">=9",
    "prettier": ">=3"
  },
  "engines": {
    "node": ">=20"
  }
}
